    "@types/node": "22.10.10",
    "@types/react": "19.0.8",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "globals": "15.14.0",
    "prettier": "3.4.2",
    "tailwindcss": "4.0.0",
    "tailwindcss-animate": "1.0.7",
